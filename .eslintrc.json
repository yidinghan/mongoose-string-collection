{
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": [
            "error",
            {
                "arrays": "only-multiline",
                "objects": "only-multiline",
                "functions": "never"
            }
        ],
        "no-param-reassign": 0
    },
    "plugins": [
        "import"
    ]
}